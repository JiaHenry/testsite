  "license": "ISC",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "body-parser": "^1.14.2",
    "bootstrap": "^3.3.6",
    "express": "^4.13.3",
    "graphql": "^0.4.14",
    "history": "^2.0.0-rc1",
    "pg": "^4.4.3",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-router": "^1.0.3",
    "reqwest": "^2.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "clean-css": "^3.4.9",
    "nodemon": "^1.8.1",
    "rework": "^1.0.1",
    "rework-npm": "^1.0.0",
    "rework-npm-cli": "^0.1.1",
    "uglify-js": "^2.6.1"
