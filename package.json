  "description": "Internal Application",
  "main": "app.js",
  "scripts": {
    "build": "browserify --extension=.jsx --extension=.js client/app.jsx -o client/public/build/build.js",
    "build-css": "rework-npm index.css | cleancss -o client/public/build/build.css",
    "start": "npm run build && npm run build-css && nodemon app.js -e jsx,html --exec babel-node --presets \"es2015\"",
