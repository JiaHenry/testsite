{
  "name": "wwwhive",
  "version": "1.0.0",
  "description": "Internal Application",
  "main": "app.js",
  "scripts": {
    "build": "webpack -p --config server/webpack.config.babel.js",
    "build-css": "rework-npm index.css | cleancss -o client/public/build/build.css",
    "deploy": "npm run build && npm run build-css", 
    "start": "npm run build-css && nodemon server/app.js --exec babel-node",
    "live": "NODE_ENV=production nodemon server/app.js --exec babel-node",
    "live-win": "SET NODE_ENV=production&& nodemon server/app.js --exec babel-node",
    "debug": "npm run build-css && babel server -d es5 -s && nodemon es5/app.js --exec babel-node",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Henry Jia",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.3.17",
    "body-parser": "^1.14.2",
    "express": "^4.13.3",
    "express-graphql": "^0.4.5",
    "graphql": "^0.4.14",
    "pg": "^4.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.17.3"
  },
  "devDependencies": {
    "bootstrap": "^3.3.6",      
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "clean-css": "^3.4.9",
    "css-loader": "^0.23.1",
    "history": "^1.17.0",
    "http-proxy": "^1.12.0",
    "nodemon": "^1.8.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-router": "^1.0.3",
    "reqwest": "^2.0.5",
    "rework": "^1.0.1",
    "rework-npm": "^1.0.0",
    "rework-npm-cli": "^0.1.1",
    "style-loader": "^0.13.0",
    "uglify-js": "^2.6.1",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  }
